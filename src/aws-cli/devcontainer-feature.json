{
    "name": "AWS CLI",
    "id": "aws-cli",
    "documentationURL": "https://github.com/j2udev/devcontainer-features/tree/main/src/aws-cli",
    "licenseURL": "https://github.com/j2udev/devcontainer-features/blob/main/LICENSE",
    "version": "0.1.0",
    "description": "Install AWS CLI",
    "options": {
        "bin_dir": {
            "type": "string",
            "proposals": [
              "/usr/local/bin"
            ],
            "default": "/usr/local/bin",
            "description": "Select a directory that is on your PATH"
        },
        "install_dir": {
            "type": "string",
            "proposals": [
              "/opt"
            ],
            "default": "/opt",
            "description": "Select a directory where supplementary files will be saved"
        }
    }
}