{
    "name": "FZF",
    "id": "fzf",
    "documentationURL": "https://github.com/j2udev/devcontainer-features/tree/main/src/fzf",
    "licenseURL": "https://github.com/j2udev/devcontainer-features/blob/main/LICENSE",
    "version": "0.1.2",
    "description": "Install FZF",
    "options": {
        "bin_dir": {
            "type": "string",
            "proposals": [
              "/usr/local/bin"
            ],
            "default": "/usr/local/bin",
            "description": "Select a directory that is on your PATH"
