{
    "name": "Oh My Posh",
    "id": "oh-my-posh",
    "documentationURL": "https://github.com/j2udev/devcontainer-features/tree/main/src/oh-my-posh",
    "licenseURL": "https://github.com/j2udev/devcontainer-features/blob/main/LICENSE",
    "version": "0.2.2",
    "description": "Install oh-my-posh",
    "options": {
        "bin_dir": {
            "type": "string",
            "proposals": [
              "/usr/local/bin"
            ],
            "default": "/usr/local/bin",
            "description": "Select a directory that is on your PATH"
        },
        "version": {
            "type": "string",
            "proposals": ["24.17.1"],
            "default": "24.17.1",
            "description": "Select an oh-my-posh version"
        },
        "theme": {
            "type": "string",
            "proposals": [
                "bubbles",
                "catppuccin",
                "craver",
                "gruvbox",
                "j2udev",
                "robbyrussell",
                "sonicboom_light",
                "sonicboom_dark",
                "tonybaloney"
            ],
            "default": "j2udev",
            "description": "Select an oh-my-posh theme"
        }
    }
}
